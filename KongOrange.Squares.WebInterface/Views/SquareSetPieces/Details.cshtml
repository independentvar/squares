@model KongOrange.Squares.DomainClasses.SquareSetPiece

@{
    ViewBag.Title = "Details";
}

<h2>Details</h2>

<div>
    <h4>SquareSetPiece</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.ImageUrl)
        </dt>

        <dd>
            <img src="@Html.DisplayFor(model => model.ImageUrl)" alt="piece" width="300" height="300" />
        </dd>

    </dl>
</div>
<p>
    @Html.ActionLink("Edit", "Edit", new {id = Model.Id}) |
    @Html.ActionLink("Delete", "Delete", new { id = Model.Id, squareSetId = Model.SquareSetId }) |
    @Html.ActionLink("Back to List", "Details", "SquareSets", new { id = Model.SquareSetId }, null)
</p>
